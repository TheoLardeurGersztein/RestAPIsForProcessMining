		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
			<version>3.12.0</version>
		</dependency>

		<dependency>
		    <groupId>org.processmining.plugins.InductiveMiner</groupId>
		    <artifactId>InductiveMiner</artifactId>
		    <version>1.0</version>
		    <scope>system</scope>
