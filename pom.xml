		<dependency>
			<groupId>org.processmining.plugins.InductiveMinerDeprecated</groupId>
			<artifactId>InductiveMinerDeprecated</artifactId>
			<version>1.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/InductiveMinerDeprecated-latest.jar</systemPath>
		</dependency>

		<dependency>
			<groupId>org.processmining.acceptingpetrinet</groupId>
			<artifactId>acceptingPetrinet</artifactId>
			<version>1.0.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/AcceptingPetriNet-latest.jar</systemPath>
		</dependency>

		<dependency>
			<groupId>org.processmining.contexts</groupId>
			<artifactId>context</artifactId>
			<version>1.0.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/ProM-Contexts-latest.jar</systemPath>
		</dependency>

		<dependency>
			<groupId>org.processmining.framework</groupId>
			<artifactId>prom-framwork</artifactId>
			<version>1.0.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/lib/ProM-Framework-latest.jar</systemPath>
		</dependency>

		<dependency>
			<groupId>org.processmining.plugins.BPMN</groupId>
